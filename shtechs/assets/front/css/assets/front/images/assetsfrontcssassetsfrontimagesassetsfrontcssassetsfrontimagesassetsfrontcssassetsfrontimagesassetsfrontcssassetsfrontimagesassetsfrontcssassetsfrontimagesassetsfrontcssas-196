<script>

    function pkg(category = "", lead_package = "", lead_amount = 0 ){

        $('[name=lead_amount]').val(lead_amount);

        $('[name=lead_package]').val(lead_package);

        $('[name=category]').val(category);

    }

    

    

    $('#exampleModal-01').on('hidden.bs.modal', function () {

      pkg('','','');

    });

    

    

    $('#exampleModal').on('hidden.bs.modal', function () {

      pkg('','','');

    });

    

$('.chat').click( function(){

    zE.activate();

});



zE(function() {

    $zopim(function() {

      $zopim.livechat.setOnUnreadMsgs(unread);

        function unread(number) {

        if (number>=1)

        {

      $zopim.livechat.window.show();

        }

        }

    });

});



    
window.onload = function(){
  
      zE.activate();
  
};

</script>



